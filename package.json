{
  "name": "react-hot-env",
  "version": "1.0.0",
  "description": "A boilerplate as simple as possible. And it works!",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=production babel src/components --out-dir lib && ./node_modules/.bin/babel-node ./test/index.js | tap-spec",
    "start": "NODE_ENV=development node server.js",
    "build": "./node_modules/.bin/rimraf build && NODE_ENV=production ./node_modules/.bin/webpack",
    "travis": "npm run coveralls",
    "prepublish": "npm run build",
    "coverage": "NODE_ENV=production babel src/components --out-dir lib && ./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --dir cov ./test/index.js",
    "coveralls": "npm run coverage && ./node_modules/.bin/coveralls < cov/lcov.info && ./node_modules/.bin/rimraf ./cov",
    "clean": "./node_modules/.bin/rimraf build cov lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isnifer/react-hot-env.git"
  },
  "keywords": [
    "webpack",
    "express",
    "hot",
    "react",
    "postcss"
  ],
  "author": "Anton Kuznetsov <isnifer@gmail.com> (https://github.com/isnifer)",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/isnifer/react-hot-env/issues"
  },
  "homepage": "https://github.com/isnifer/react-hot-env#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.25.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.4",
    "istanbul": "^0.4.3",
    "jsdom": "^9.8.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.1.0",
    "react-addons-test-utils": "^15.0.2",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
